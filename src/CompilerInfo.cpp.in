#include "CompilerInfo.hpp"

/// Git specific info

const char CompilerInfo::_git_build_string[] = "@GIT_BUILD_STRING@";

/// Compilation time info

const uint_least32_t CompilerInfo::_compilation_time_day =
    atoi("@COMPILATION_TIME_DAY@");
const uint_least32_t CompilerInfo::_compilation_time_month =
    atoi("@COMPILATION_TIME_MONTH@");
const uint_least32_t CompilerInfo::_compilation_time_year =
    atoi("@COMPILATION_TIME_YEAR@");
const uint_least32_t CompilerInfo::_compilation_time_hour =
    atoi("@COMPILATION_TIME_HOUR@");
const uint_least32_t CompilerInfo::_compilation_time_minutes =
    atoi("@COMPILATION_TIME_MINUTES@");
const uint_least32_t CompilerInfo::_compilation_time_seconds =
    atoi("@COMPILATION_TIME_SECONDS@");

// Compiler info

const char CompilerInfo::_compiler_name[] = "@COMPILER_NAME@";
const char CompilerInfo::_compiler_version[] = "@COMPILER_VERSION@";

/// Operating system info

const char CompilerInfo::_os_name[] = "@OS_NAME@";
const char CompilerInfo::_os_kernel_name[] = "@OS_KERNEL_NAME@";
const char CompilerInfo::_os_kernel_release[] = "@OS_KERNEL_RELEASE@";
const char CompilerInfo::_os_kernel_version[] = "@OS_KERNEL_VERSION@";
const char CompilerInfo::_os_hardware_name[] = "@OS_HARDWARE_NAME@";
const char CompilerInfo::_os_host_name[] = "@OS_HOST_NAME@";
